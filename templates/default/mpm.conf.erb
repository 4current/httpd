<IfModule <%= @config.parsed_mpm %>_module>
<% if @config.parsed_startservers %>
  StartServers           <%= @config.parsed_startservers %>
<% end %>
<% if @config.parsed_minspareservers %>
  MinSpareServers        <%= @config.parsed_minspareservers %>
<% end %>
<% if @config.parsed_maxspareservers %>
  MaxSpareServers        <%= @config.parsed_maxspareservers %>
<% end %>
<% if @config.parsed_maxclients %>
  MaxClients             <%= @config.parsed_maxclients %>
<% end %>
<% if @config.parsed_maxrequestsperchild %>
  MaxRequestsPerChild    <%= @config.parsed_maxrequestsperchild %>
<% end %>
<% if @config.parsed_minsparethreads %>
  MinSpareThreads        <%= @config.parsed_minsparethreads %>
<% end %>
<% if @config.parsed_maxsparethreads %>
  MaxSpareThreads        <%= @config.parsed_maxsparethreads %>
<% end %>
<% if @config.parsed_threadlimit %>
  ThreadLimit            <%= @config.parsed_threadlimit %>
<% end %>
<% if @config.parsed_threadsperchild %>
  ThreadsPerChild        <%= @config.parsed_threadsperchild %>
<% end %>
<% if @config.parsed_maxrequestworkers %>
  MaxRequestWorkers      <%= @config.parsed_maxrequestworkers %>
<% end %>
<% if @config.parsed_maxconnectionsperchild %>
  MaxConnectionsPerChild <%= @config.parsed_maxconnectionsperchild %>
<% end %>
</IfModule>
